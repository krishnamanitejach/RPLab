//1.Write a program to implement Type Casting Operator
fn main(){
    let a: i32 = 10;
    let b: f64 = a as f64;

    println!("Integer value: {}", a);
    println!("Cast to float: {}", b);

    let x: f64 = 3.14;
    let y: i32 = x as i32;

    println!("Float value: {}", x);
    println!("Cast to integer: {}",y);
}
